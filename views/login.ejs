<body>
    <div class="container">
    <a href="/">Home</a>

<form action="/users/login" method="post">
    <div class="form-group">
        <input type="email" class="form-control" name="email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter registered email">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <input type="password" class="form-control" name="password" id="exampleInputPassword1" placeholder="Enter registered password">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

</div>
</body>
